    "test:unit": "rimraf coverage && jest --coverage",
    "test:unit:open": "rimraf coverage && jest --coverage --watch",
    "test:integration": "cypress run --browser chrome",
    "test:integration:open": "cypress open",
    "precommit": "lint-staged",
    "pretty": "pretty-quick",
    "preparecommitmsg": "commit-template ./internals/templates/git/git-commit-template.txt",
    "gen:duck": "node internals/scripts/ducks-boiler.js",
    "gen:page": "node internals/scripts/pages-boiler.js",
    "gen:unit": "node internals/scripts/units-boiler.js",
    "clean:build": "rimraf build/",
    "clean:cover": "rimraf coverage/",
    "reinstall:all": "rimraf node_modules && rimraf package-lock.json && npm install"
